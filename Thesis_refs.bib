	author = {Ghose, Romi and Omoluabi, Ozozoma and Gandhi, Adarsh and Shah, Pranav and Strohacker, Kelley and Carpenter, Katie C and McFarlin, Brian and Guo, Tao},
	doi = {10.1016/j.lfs.2011.05.005},
