	issn = {1879-0631},
	journal = {Life sciences},
	month = {jul},
	number = {1-2},
	pages = {57--64},
	pmid = {21620874},
	publisher = {NIH Public Access},
	title = {{Role of high-fat diet in regulation of gene expression of drug metabolizing enzymes and transporters.}},
