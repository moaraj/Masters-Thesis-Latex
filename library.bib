issn = {1744-4292},
journal = {Molecular systems biology},
month = {nov},
pages = {548},
pmid = {22068331},
publisher = {European Molecular Biology Organization},
title = {{Deep proteome and transcriptome mapping of a human cancer cell line.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22068331 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3261714},
volume = {7},
year = {2011}
}
@article{Escher2012,
abstract = {Multiple reaction monitoring (MRM) has recently become the method of choice for targeted quantitative measurement of proteins using mass spectrometry. The method, however, is limited in the number of peptides that can be measured in one run. This number can be markedly increased by scheduling the acquisition if the accurate retention time (RT) of each peptide is known. Here we present iRT, an empirically derived dimensionless peptide-specific value that allows for highly accurate RT prediction. The iRT of a peptide is a fixed number relative to a standard set of reference iRT-peptides that can be transferred across laboratories and chromatographic systems. We show that iRT facilitates the setup of multiplexed experiments with acquisition windows more than four times smaller compared to in silico RT predictions resulting in improved quantification accuracy. iRTs can be determined by any laboratory and shared transparently. The iRT concept has been implemented in Skyline, the most widely used software for MRM experiments.},
author = {Escher, Claudia and Reiter, Lukas and MacLean, Brendan and Ossola, Reto and Herzog, Franz and Chilton, John and MacCoss, Michael J and Rinner, Oliver},
doi = {10.1002/pmic.201100463},
file = {:C$\backslash$:/Users/Moaraj Hasan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Escher et al. - 2012 - Using iRT, a normalized retention time for more targeted measurement of peptides.pdf:pdf},
issn = {1615-9861},
journal = {Proteomics},
month = {apr},
number = {8},
pages = {1111--21},
pmid = {22577012},
publisher = {NIH Public Access},
title = {{Using iRT, a normalized retention time for more targeted measurement of peptides.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22577012 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3918884},
volume = {12},
year = {2012}
}
